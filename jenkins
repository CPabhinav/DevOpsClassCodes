pipeline {     agent any
    tools {
        maven 'my-maven'
    }
    
    stages {         stage('Clone') {
            steps {
                echo 'Cloning the repository...'
                git branch: 'master', url: 'https://github.com/CPabhinav/DevOpsClassCodes.git'
            }
        }
       stage('Compile') {             steps {
                echo 'Compiling the code...'
                sh 'mvn clean compile'
            }
        }

        }
        }
